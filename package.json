{
  "name": "jag-reehal-stencil-tutorial",
  "version": "0.0.1",
  "description": "Stencil Tutorial created by Jag Reehal",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.10.0-3",
    "@types/jest": "^23.1.2",
    "jest": "^23.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagreehal/jag-reehal-stencil-tutorial.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jagreehal/jag-reehal-stencil-tutorial"
  },
  "homepage": "https://github.com/jagreehal/jag-reehal-stencil-tutorial",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
